{
    "version": 2,
    "functions": {
      "maxDuration": 30 
    },
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" },
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/(.*)", "dest": "api/$1" },
      { "src": "/(.*)", "dest": "server.js" }
    ]
  }
  